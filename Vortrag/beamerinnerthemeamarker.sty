  \includegraphics[trim={5 0 45 1}, clip, scale=0.5]{FHAAC_4C_BL-r}%
  %}%
}
\setbeamertemplate{title page}{%
  \vspace*{-1pt}
  \begin{beamercolorbox}[ht=\paperheight, dp=0pt, wd=\paperwidth,
      sep=0.03\paperwidth, left]{title}
      \insertlogo%
      \hspace*{-0.03\paperwidth}%
      \vbox to 0.25\paperheight{% changed from 0.37!!!
        {\usebeamerfont{title}\inserttitle}%
      }%
      \par\vspace{0.01\paperheight}
      \vbox to 0.33\paperheight{% changed from 0.21!!
        \ifx\insertsubtitle\@empty\else%
        {\usebeamerfont{subtitle}%
          \insertsubtitle\par}%
        \fi%
        {\usebeamerfont{date}\insertdate}%
        \par\vspace{10pt}%
        {\usebeamerfont{author}\insertauthor}%
      }
  \end{beamercolorbox}%
}
\def\subsubsectionpage{\usebeamertemplate*{subsubsection page}}
\mode
<article>
{\providecommand\subsubsectionpage{}}

\defbeamertemplate*{subsubsection page}{default}[1][]{%
  \begin{beamercolorbox}[sep=8pt,center,#1]{subsubsection title}
    \usebeamerfont{subsubsection title}\insertsubsubsectionhead\par
  \end{beamercolorbox}
}

\setbeamertemplate{subsubsection page}[default][colsep=-4bp,
    rounded=true,shadow=\beamer@themerounded@shadow]
\setbeamertemplate{itemize item}{\textgreater}
\setbeamertemplate{itemize subitem}{\textgreater}
\setbeamertemplate{itemize subsubitem}{\textgreater}
\mode
<all>
